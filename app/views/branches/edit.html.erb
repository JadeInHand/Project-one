<div class="info">
	<h1> EDIT PAGE </h1>

	<%= form_for @branch do |f| %>
		<p>
			<%= f.label :name %>
			<%= f.text_field :name %>
		</p>
		<p>
			<%= f.label :functions %>
			<%= f.text_field :functions %>
		</p>
		<p>
			<%= f.label :powers %>
			<%= f.text_field :powers %>
		</p>
		<p>
			<%= f.label :history %>
			<%= f.text_field :history %>
		</p>
		<p>
			<%= f.submit %>
		</p>
	<% end %>
</div>


